<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="195.0" prefWidth="195.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="resultsComponent.executionDetails.buttons.RunningController">
   <children>
      <Button fx:id="pauseButton" mnemonicParsing="false" onAction="#pauseButtonClick" prefHeight="30.0" prefWidth="100.0" styleClass="button-show" text="Pause" />
      <Button fx:id="resumeButton" mnemonicParsing="false" onAction="#resumeButtonClick" prefHeight="30.0" prefWidth="100.0" styleClass="button-show" text="Resume" />
      <Button fx:id="stopButton" mnemonicParsing="false" onAction="#stopButtonClick" prefHeight="30.0" prefWidth="100.0" styleClass="button-show" text="Stop" />
      <Button fx:id="futureButton" mnemonicParsing="false" onAction="#FutureButtonClick" prefHeight="30.0" prefWidth="100.0" styleClass="button-show" text="Future tick" />
   </children>
</VBox>
