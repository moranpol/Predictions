   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="980.0" spacing="10.0">
         <children>
            <ComboBox fx:id="worldCBox" prefWidth="150.0" promptText="Choose world" styleClass="ComboBox" />
            <Button fx:id="showWorldButton" mnemonicParsing="false" onAction="#showWorldButtonClicked" styleClass="button-show" text="Show" />
         </children>
         <VBox.margin>
            <Insets left="20.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <Pane fx:id="detailsPane" prefHeight="500.0" prefWidth="1000.0" />
   </children>
</VBox>
